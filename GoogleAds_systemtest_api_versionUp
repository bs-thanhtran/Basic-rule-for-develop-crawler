                    content_report, content_creative, content_conversion_action),
                    array_length(creative_image_ad_image_url),
                    array_length(creative_responsive_display_ad_logo_images_url),
                    array_length(creative_responsive_display_ad_square_marketing_images_url),
                    array_length(creative_app_ad_images_url),
                    array_length(creative_app_engagement_ad_images_url),
